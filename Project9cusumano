//Project 9 Tara Cusumano

Fish Tara;

float x,y,w,h;
float horizon;



class Fish{
    //properties
    float x=200,y=-300,dx=100,dy=200;
    float width=30;
    float r=200,g=150,b=150;
    int hands=1;
    int num;
}

void setup(){
  background(255,200,200);
  size(640,480);
  horizon=height/2;
  
  Tara= new Fish();
  Tara.r=200;
  Tara.g=150;
  Tara.b=150;

reset();
}
void reset(){
Tara.hands=1;

 


}
